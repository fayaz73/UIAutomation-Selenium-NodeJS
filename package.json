{
  "name": "mocha-browserstack",
  "version": "0.0.1",
  "description": " Selenium examples for Mocha and BrowserStack Automate",
  "scripts": {
    "test1": "npx mocha --reporter mochawesome spec ./test/segmenttest.spec.js --exit",
    "test": "npx mocha --reporter allure-mocha spec ./test/segmenttest.spec.js --exit",
    "report": "allure generate allure-results --clean && allure open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Immensitas/ui-test-automation.git"
  },
  "keywords": [
    "mocha",
    "browserstack",
    "selenium",
    "automate",
    "examples"
  ],
  "license": "ISC",
  "dependencies": {
    "browsermob-proxy": "^1.0.13",
    "browserstack-local": "^1.4.8",
    "browserstack-webdriver": "^2.41.1",
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "google-spreadsheet": "^4.1.1",
    "googleapis": "^129.0.0",
    "mocha-selenium": "^0.1.1",
    "properties-reader": "^2.3.0",
    "selenium-webdriver": "^4.18.1"
  },
  "devDependencies": {
    "allure-commandline": "^2.26.0",
    "allure-mocha": "^2.12.2",
    "browsermob-proxy-client": "^0.1.2",
    "browserstack-node-sdk": "latest",
    "mocha": "^10.3.0",
    "mocha-allure-reporter": "^1.4.0",
    "mochawesome": "^7.1.3",
    "monocart-reporter": "^2.3.1"
  },
  "mocha": {
    "parallel": false,
    "reporter": "allure-mocha",
    "reporterOptions": {
      "resultsDir": "allure-results"
    }
  }
}

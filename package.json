{
  "name": "mocha-browserstack",
  "version": "0.0.1",
  "description": " Selenium examples for Mocha and BrowserStack Automate",
  "scripts": {
    "sample-test": "browserstack-node-sdk mocha test/sample_test.js --timeout=600000",
    "sample-local-test": "browserstack-node-sdk mocha test/sample_local_test.js --timeout=600000",
    "execute-testCases": "browserstack-node-sdk mocha test/excuteTC.js --timeout=600000",
    "testCase": "browserstack-node-sdk mocha test/logintest.spec.js --timeout=600000",
    "testCase1": "browserstack-node-sdk mocha test/logintest.spec.js --timeout=600000",
    "postinstall": "npm update browserstack-node-sdk",
    "wdio": "wdio run ./wdio.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/browserstack/mocha-browserstack.git"
  },
  "keywords": [
    "mocha",
    "selenium",
    "automate",
    "examples"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/browserstack/mocha-browserstack/issues"
  },
  "homepage": "https://github.com/browserstack/mocha-browserstack#readme",
  "dependencies": {
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "mocha": "^9.2.2",
    "mocha-allure-reporter": "^1.4.0",
    "properties-reader": "^2.3.0",
    "selenium-webdriver": "^4.16.0"
  },
  "devDependencies": {
    "allure-commandline": "^2.26.0",
    "allure-mocha": "^2.11.1",
    "browsermob-proxy-client": "^0.1.2",
    "browserstack-node-sdk": "latest"
  },
  "mocha": {
    "parallel": false,
    "reporter": "allure-mocha",
    "reporterOptions": {
      "resultsDir": "allure-results"
    }
  }
}
